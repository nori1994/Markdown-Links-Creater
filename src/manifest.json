{
    "manifest_version": 2,
    "name": "Markdown Links Creater",
    "version": "0.1",
    "description": "Copy the selected text as markdown.",
    "icons": {},
    "browser_action": {
        "default_icon": {}
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "options_page": "options.html",
    "permissions": [
        "tabs",
        "contextMenus",
        "storage"
    ]
}