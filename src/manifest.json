{
    "manifest_version": 2,
    "name": "Markdown Links Creater",
    "version": "0.1",
    "description": "Create link as Markdown using selected text or title of tab, and stocke created link. Then copy stocked Link to ClipBoard.",
    "icons": {},
    "browser_action": {
        "default_icon": {}
    },
    "background": {
        "scripts": [
            "js/common.js",
            "js/background.js"
        ],
        "persistent": false
    },
    "options_page": "options.html",
    "permissions": [
        "tabs",
        "contextMenus",
        "storage"
    ]
}